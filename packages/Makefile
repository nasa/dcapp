.NOTPARALLEL:
.PHONY: all clean

all:
	${MAKE} -C basicutils
	${MAKE} -C logicutils
	${MAKE} -C TaraDraw
	${MAKE} -C RenderLib
	${MAKE} -C PixelStream

clean:
	${MAKE} -C basicutils clean
	${MAKE} -C logicutils clean
	${MAKE} -C TaraDraw clean
	${MAKE} -C PixelStream clean
	${MAKE} -C RenderLib clean
